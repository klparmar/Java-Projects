import java.awt.*;
import java.awt.event.*;
import java.awt.Rectangle;

public class Enemy {
  
  /* ADD YOUR CODE HERE */
  int xPos, yPos = 550;
  int Cap;
  boolean direction = true;
  int speed = 1;
  Rectangle hitBox;
  
  public void move(){
    if(direction){
      xPos+=speed;
      if(xPos==750){
        xPos = -100;
      }
    }
    if(!direction){
      xPos-=speed;
      if(xPos==-100){
        xPos = 750;
      }
    }
  }
  
}

